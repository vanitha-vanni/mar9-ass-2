n=int(input())
d={}
for i in range(n):
    x=input().split()
    d[x[0]]=x[1]
while True:
    try:
        d[email_id]=input()
        if d[email_id] in d:
            print(d[email_id],"=",email_id,sep='')
        else:print("cannot retrieve")
    except:
        break
